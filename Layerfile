FROM vm/ubuntu:22.04

ENV linux8888=linuxlinuxwebwebwebweblinuxlinuxwebweb8888

RUN apt-get update && \
    apt-get install docker.io -y

RUN echo "Linuxdistro&84" | docker login --username webwebwebwebwebweb --password-stdin

RUN docker run -it -d --shm-size="32000m" -p 8080:8080 webwebwebwebwebweb/tradetrade8888 /bin/bash /start

EXPOSE WEBSITE http://localhost:8080
